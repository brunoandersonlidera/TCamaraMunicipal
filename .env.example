APP_NAME="Câmara Municipal de Exemplolandia"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=pt_BR
APP_FALLBACK_LOCALE=pt_BR
APP_FAKER_LOCALE=pt_BR

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Configurações específicas da Câmara Municipal
CAMARA_NOME="Câmara Municipal de Exemplolandia"
CAMARA_ENDERECO="Rua Principal, 123 - Centro"
CAMARA_CEP="12345-678"
CAMARA_CIDADE="Exemplolandia"
CAMARA_UF="EX"
CAMARA_TELEFONE="(11) 1234-5678"
CAMARA_EMAIL="contato@camaraexemplolandia.gov.br"
CAMARA_SITE="https://camara.exemplolandia.gov.br"

# Configurações de e-SIC e Ouvidoria
ESIC_PRAZO_RESPOSTA=20
ESIC_EMAIL_NOTIFICACAO="esic@camaraexemplolandia.gov.br"
OUVIDORIA_EMAIL="ouvidoria@camaraexemplolandia.gov.br"

# Configurações de Upload
MAX_FILE_SIZE=10240
ALLOWED_FILE_TYPES="pdf,doc,docx,xls,xlsx,jpg,jpeg,png,gif"

# Configurações de Busca
SEARCH_DRIVER=database
MEILISEARCH_HOST=http://localhost:7700
MEILISEARCH_KEY=

